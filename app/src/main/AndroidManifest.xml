<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.zmt.photometerapp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/zmt_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        android:supportsRtl="true"
        <activity
            android:name=".MainLayout"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondLayer"
            android:label="Photometer App" />
        <activity
            android:name=".Help"
            android:label="Help" />
        <activity
            android:name=".Database"
            android:label="Database" />
        <activity
            android:name=".Parameter"
            android:label="Parameter Selection" />
        <activity
            android:name=".Measurement"
            android:label="Measurement" />
        <activity
            android:name=".Calibration"
            android:label="New Calibration" />
        <activity
            android:name=".StoredCalibration"
            android:label="Stored Calibration" />
        <activity android:name=".DataListActivity"></activity>
        <activity
            android:name=".BlankActivity"
            android:label="Blank Activity" />
        <activity android:name=".phosphate_activity_class"></activity>
    </application>

</manifest>